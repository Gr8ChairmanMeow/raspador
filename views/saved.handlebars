<div class="container">
    <div class="jumbotron">
        <h1>Saved Articles</h1>
        <button class="btn-lg btn-danger">Home</button>
    </div>
</div>
<div id="wrapper">
    <div id="articles">
        <h2>Articles <!-- <img src="assets/images/burger.png" alt="eat" width="40"/> --></h2>
        <hr>
        <ul>
            {{#each articles}}  {{#if this.saved}}
            <li>
                <p data-id={{this._id}} class="articleNote"> {{this.title}}
                <br />
               {{> save-unsave}}
                <button class="btn btn-add" data-id={{this._id}}>Add Comments</button>
                <button class="btn btn-comments" data-id={{this._id}}>View Comments</button>
                <br />
                <a href={{this.link}} target="_blank">{{this.link}}</a></p>
            </li>
            {{/if}} {{/each}}
        </ul>
    </div>
    <div id="notes"></div>
</div>